    "react-native-dropdownalert": "^3.10.0",
    "react-native-elements": "^1.1.0",
    "react-native-geolocation-service": "^4.0.0",
    "react-native-gesture-handler": "1.6.1",
    "react-native-image-crop-picker": "^0.28.0",
    "react-native-keychain": "6.0.0",
    "react-native-login-keycloak": "https://github.com/jonatasdlp/react-native-login-keycloak",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-material-menu": "^1.1.0",
    "react-native-material-ui": "^1.30.1",
    "react-native-modal": "^11.5.4",
    "react-native-modalbox": "^1.7.1",
    "react-native-permissions": "^2.0.8",
    "react-native-push-notification": "^3.1.9",
    "react-native-qrcode-scanner": "^1.2.1",
    "react-native-raw-bottom-sheet": "^2.0.6",
    "react-native-reanimated": "^1.7.1",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.0.0-alpha.32",
    "react-native-shake": "^3.4.0",
    "react-native-share": "^3.5.0",
    "react-native-spinkit": "^1.2.0",
    "react-native-svg": "^9.4.0",
    "react-native-tcp-socket": "^3.5.1",
    "react-native-user-agent": "^1.0.4",
    "react-native-version-number": "^0.3.6",
    "react-native-view-shot": "^3.1.2",
    "react-navigation": "^4.3.7",
    "react-navigation-drawer": "^2.4.11",
    "react-navigation-stack": "^2.3.11",
    "react-navigation-tabs": "^2.8.11",
    "react-redux": "^6.0.1",
    "react-relay": "^9.0.1",
    "react-relay-network-modern": "^4.6.0",
    "react-relay-offline": "^2.2.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
