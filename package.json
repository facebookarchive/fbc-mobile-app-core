{
  "name": "fbc-mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -o reports/flow/",
    "test-coverage": "jest --ci --runInBand --collectCoverage --coverageDirectory=./reports/junit/",
    "prettier-fix": "prettier --write **/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "lint-coverage": "eslint . --format junit -o reports/lint/js-lint-results.xml",
    "rename": "react-native-rename",
    "install:ctf": "export ENVFILE=.env.ctf && react-native run-android --variant=CtfDebug --appId=com.facebook.ctf",
    "install:terragraph": "export ENVFILE=.env.terragraph && react-native run-android --variant=TerragraphDebug --appId=com.terragraph",
    "assemble:terragraph": "cd android/ && export ENVFILE=.env.terragraph && $GRADLEW_BINARY -Dhttps.proxyHost=fwdproxy -Dhttps.proxyPort=8080 assembleTerragraphRelease -PappVersionName=1.0.2 -PkeystoreFile=fb/tg-keystore.properties && cd ..",
    "release:terragraph": "cd android/ && export ENVFILE=.env.terragraph && ./gradlew assembleTerragraphRelease -PappVersionName=1.0.2 -PenableSeparateBuildPerCPUArchitecture=true -PkeystoreFile=fb/tg-keystore.properties && cd ..",
    "install:platform": "echo 'local debug build'   && export ENVFILE=.env.platform && react-native run-android --appId=cloud.thesymphony --variant=PlatformDebug ",
    "install:releasePlatform": "echo 'local release build' && export ENVFILE=.env.platform && react-native run-android --appId=cloud.thesymphony --variant=PlatformRelease",
    "assemble:platform": "echo 'sandcastle signed release build'     && cd android/ && export ENVFILE=.env.platform && $GRADLEW_BINARY -Dhttps.proxyHost=fwdproxy -Dhttps.proxyPort=8080 assemblePlatformRelease -PappVersionName=1.0.19 -PkeystoreFile=fb/symphony-keystore.properties && cd ..",
    "release:platform": "echo 'local signed release build'           && cd android/ && export ENVFILE=.env.platform && ./gradlew --stacktrace assemblePlatformRelease -PappVersionName=1.0.19 -PenableSeparateBuildPerCPUArchitecture=true -PkeystoreFile=fb/symphony-keystore.properties && cd ..",
    "circleci:platform": "echo 'circleci debug-signed release build' && cd android/ && export ENVFILE=.env.platform && ./gradlew --stacktrace assemblePlatformRelease -PappVersionName=1.0.19 -PenableSeparateBuildPerCPUArchitecture=true -PkeystoreFile=keystores/circleci-keystore.properties && cd ..",
    "relay": "echo '\n\n\n****\nRemember to git pull your ~/symphony/ repo! \n****\n\n\n' && cp ~/symphony/graph/graphql/schema/symphony.graphql src/Platform/Relay/symphony.graphql && relay-compiler --src src/ --schema src/Platform/Relay/symphony.graphql && node scripts/markGeneratedFiles.js",
    "relay:watch": "yarn relay --watch",
    "manifest": "babel-node ./node_modules/babel-plugin-fbt/bin/manifest --src src/ packages/ --enum-manifest i18n/fbt/.enum_manifest.json --src-manifest i18n/fbt/.src_manifest.json",
    "collect-fbts": "babel-node ./node_modules/babel-plugin-fbt/bin/collectFBT --hash-module 'fb-tiger-hash/src/hashPhrases' --react-native-mode --manifest < i18n/fbt/.src_manifest.json > i18n/fbt/.source_strings.json",
    "extract-drop-fbt": "babel-node scripts/fb/i18n/extract-and-drop --node node",
    "fetch-land-fbt": "babel-node scripts/fb/i18n/fetch-land-translations --node node",
    "proto": "node ./scripts/protoc.js && node scripts/markGeneratedFiles.js"
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "globExcludePatterns": [
      "src/**/__generated__/*.js",
      "src/Terragraph/NodeAlignment/ProtocolMessage/Message_pb.js"
    ],
    "threshold": 70,
    "concurrentFiles": 100,
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  },
  "dependencies": {
    "@99xt/first-born": "^1.1.8",
    "@fbcmobile/signalscan": "file:./packages/@fbcmobile-signalscan",
    "@fbcmobile/ui": "1.0.0",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/datetimepicker": "^2.3.2",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/netinfo": "^4.7.0",
    "@react-native-mapbox-gl/maps": "^8.0.0",
    "@turf/helpers": "^6.1.4",
    "@turf/turf": "^5.1.6",
    "@use-it/interval": "^0.1.3",
    "apisauce": "^1.0.2",
    "beaver-logger": "^4.0.12",
    "buffer": "^5.5.0",
    "crc": "^3.8.0",
    "flowtype-protoc-gen": "^0.8.1-pre",
    "geolib": "^3.3.1",
    "mcc-mnc-list-js": "^1.0.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
