{
  "name": "fbc-mobile-app-core",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -o reports/flow/",
    "test-coverage": "jest --ci --runInBand --collectCoverage --coverageDirectory=./reports/junit/",
    "prettier-fix": "prettier --write **/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "lint-coverage": "eslint . --format junit -o reports/lint/js-lint-results.xml"
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "packages/**/*.js"
    ],
    "threshold": 70,
    "concurrentFiles": 100,
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.1",
    "@babel/runtime": "^7.6.2",
    "@kironeducation/flow-junit-transformer": "^0.3.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^5.16.0",
    "eslint-config-fb-strict": "^24.9.0",
    "eslint-config-fbcnms": "^0.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-lint": "^1.0.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.1.1",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "flow-bin": "^0.134.0",
    "flow-coverage-report": "^0.8.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "jest-junit": "^10.0.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "mock-async-storage": "^2.2.0",
    "prepend-file": "^1.3.1",
    "prettier": "1.19.1",
    "react-native-vector-icons": "7.0.0",
    "react-test-renderer": "16.11.0"
  },
  "resolutions": {
    "@react-native-community/cli-debugger-ui": "4.9.0",
    "yargs-parser": "20.2.2"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {}
}
