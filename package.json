    "@babel/runtime": "^7.6.2",
    "@kironeducation/flow-junit-transformer": "^0.3.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-fbt": "^0.14.1",
    "babel-plugin-fbt-runtime": "^0.9.9",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-relay": "^8.0.0",
    "babel-preset-react-native": "^4.0.1",
    "create-react-class": "^15.6.3",
    "eslint": "^5.16.0",
    "eslint-config-fb-strict": "^24.9.0",
    "eslint-config-fbcnms": "^0.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-lint": "^1.0.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.1.1",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-relay": "^1.3.10",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fb-tiger-hash": "^0.1.6",
    "fbt": "^0.14.0",
    "flow-bin": "^0.134.0",
    "flow-coverage-report": "^0.7.0",
